ControlPath ~/.ssh/control/control-%h-%p-%r
ControlPersist yes
ControlMaster auto
ServerAliveInterval 60
HashKnownHosts yes
AddKeysToAgent yes


Host github.com
    Hostname github.com
    IdentityFile ~/.ssh/id_ed25519-github
    IdentityFile ~/.ssh/id_ed25519
    IdentitiesOnly yes
    AddKeysToAgent yes
